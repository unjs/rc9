    "automd": "^0.4.3",
    "changelogen": "^0.6.2",
    "flat": "^6.0.1",
    "obuild": "^0.4.25",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.2"
