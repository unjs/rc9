{
  "name": "rc9",
  "version": "2.0.1",
  "description": "Read/Write config couldn't be easier!",
  "repository": "unjs/rc9",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
