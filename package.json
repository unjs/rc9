  "devDependencies": {
    "@types/flat": "^5.0.2",
    "@types/node": "^18.15.3",
    "@vitest/coverage-c8": "^0.29.3",
    "changelogen": "^0.5.1",
    "eslint": "^8.36.0",
    "eslint-config-unjs": "^0.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.3"
  },
  "packageManager": "pnpm@7.29.3"
