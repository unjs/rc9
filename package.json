    "eslint": "^8.49.0",
    "eslint-config-unjs": "^0.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.4"
  },
  "packageManager": "pnpm@8.7.4"
