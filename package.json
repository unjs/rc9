{
  "name": "rc9",
  "version": "2.1.2",
  "description": "Read/Write config couldn't be easier!",
  "repository": "unjs/rc9",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "obuild",
    "dev": "vitest",
    "lint": "eslint . && prettier -c src test",
